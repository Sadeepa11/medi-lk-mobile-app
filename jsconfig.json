{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "allowJs": true,
    "jsx": "react-native",
    "moduleResolution": "node",
    "noEmit": true,
    "isolatedModules": true,
    "strict": false,
    "baseUrl": ".",
    "paths": {
      "*": ["src/*", "*"]
    }
  },
  "include": ["**/*.js", "**/*.jsx"],
  "exclude": ["node_modules", "babel.config.js", "metro.config.js", "jest.config.js"]
}
